{
  "kind": "collectionType",
  "collectionName": "checkouts",
  "info": {
    "singularName": "checkout",
    "pluralName": "checkouts",
    "displayName": "Checkout",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "payment_pending",
        "completed",
        "expired"
      ]
    },
    "expiresAt": {
      "required": true,
      "type": "datetime"
    },
    "stock_reservation_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stock-reservation-item.stock-reservation-item",
      "mappedBy": "checkout"
    },
    "expired": {
      "type": "boolean"
    },
    "stripePaymentIntentId": {
      "type": "string"
    },
    "checkoutSessionId": {
      "type": "string"
    }
  }
}
