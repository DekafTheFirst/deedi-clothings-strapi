{
  "kind": "collectionType",
  "collectionName": "cart_items",
  "info": {
    "singularName": "cart-item",
    "pluralName": "cart-items",
    "displayName": "CartItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "quantity": {
      "required": true,
      "type": "integer",
      "default": 1
    },
    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "items"
    },
    "size": {
      "type": "string",
      "required": true
    },
    "localCartItemId": {
      "type": "string"
    },
    "outOfStock": {
      "type": "boolean"
    }
  }
}
