{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "string"
    },
    "stripeId": {
      "type": "text"
    },
    "products": {
      "type": "json"
    },
    "selectedCourier": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "delivered",
        "cancelled"
      ],
      "default": "pending"
    },
    "totalAmount": {
      "type": "float"
    },
    "shipping_address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shipping-address.shipping-address",
      "inversedBy": "orders"
    },
    "billing_address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::billing-address.billing-address",
      "inversedBy": "orders"
    }
  }
}
