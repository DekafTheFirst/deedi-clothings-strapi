{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "stripePaymentIntentId": {
      "type": "string"
    },
    "items": {
      "type": "json",
      "required": true
    },
    "courierId": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "delivered",
        "cancelled",
        "checkout_session_expired"
      ],
      "default": "pending",
      "required": true
    },
    "totalAmount": {
      "type": "float"
    },
    "shipmentId": {
      "type": "string"
    },
    "trackingPageUrl": {
      "type": "string"
    },
    "shipmentStatus": {
      "type": "string"
    },
    "courierName": {
      "type": "string"
    },
    "shippingCost": {
      "type": "decimal"
    },
    "currency": {
      "type": "string",
      "required": true,
      "default": "USD"
    },
    "shippingLabelUrl": {
      "type": "string"
    },
    "minDeliveryTime": {
      "type": "integer"
    },
    "maxDeliveryTime": {
      "type": "integer"
    },
    "labelGeneratedAt": {
      "type": "datetime"
    },
    "labelPaidAt": {
      "type": "datetime"
    },
    "shippingDocuments": {
      "type": "json"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "customerEmail": {
      "type": "email",
      "required": true
    },
    "checkoutSessionId": {
      "type": "uid"
    },
    "shippingAddress": {
      "type": "json",
      "required": true
    }
  }
}
