{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "stripeId": {
      "type": "string"
    },
    "items": {
      "type": "json"
    },
    "courierId": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "delivered",
        "cancelled"
      ],
      "default": "pending"
    },
    "totalAmount": {
      "type": "float"
    },
    "shipping_address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shipping-address.shipping-address",
      "inversedBy": "orders"
    },
    "billing_address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::billing-address.billing-address",
      "inversedBy": "orders"
    },
    "shipmentId": {
      "type": "string"
    },
    "trackingPageUrl": {
      "type": "string"
    },
    "shipmentStatus": {
      "type": "string"
    },
    "courierName": {
      "type": "string"
    },
    "shippingCost": {
      "type": "decimal"
    },
    "currency": {
      "type": "string"
    },
    "shippingLabelUrl": {
      "type": "string"
    },
    "minDeliveryTime": {
      "type": "integer"
    },
    "maxDeliveryTime": {
      "type": "integer"
    },
    "labelGeneratedAt": {
      "type": "datetime"
    },
    "labelPaidAt": {
      "type": "datetime"
    },
    "trackingNumber": {
      "type": "string"
    },
    "shippingDocuments": {
      "type": "json"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "customerEmail": {
      "type": "email"
    }
  }
}
